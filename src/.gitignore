#
# gitignore for all files relative to symfony project root
#

# don't automatically add these
.*
!.gitignore
# !.babelrc
# !.eslintrc.js
# !.prettierrc.js
# !.stylelintrc.yml
!/web/.htaccess_default

# php-cs-fixer
!.php_cs
/composer.phar
/tools/*.phar

# generated / logs
*.log
error_log*

# generated / misc files
/cache
/config/vite-build.*

# generated / output from Juicer (frontend build tool) 
*.juiced.js
*.juiced.css
*.min.js
*.min.css

# generated / realfavicon output
/batch/favicon/faviconData.json
/web/favicons
!/web/favicons/README.md

# generated / build of front end assets
/web/build

# npm install
/node_modules
/vite/node_modules

# admin/misc
/web/*.php
!/web/index.php
!/web/index_dev.php
!/web/index_test.php
/web/robots*

# private
/apps/backend
/batch/deploy
/web/backend
/apps/koohii/config/app.yml
/apps/koohii/config/settings.yml
